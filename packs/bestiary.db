:0,"value":10,"bonus":1,"cost":25}},"status":{"wounds":{"type":"Number","label":"Wounds","abrev":"W","value":1,"max":1},"advantage":{"type":"Number","label":"Advantage","value":0,"max":10},"criticalWounds":{"type":"Number","label":"Critical Wounds","value":0,"max":1,"descriptions":""},"sin":{"type":"Number","label":"Sin","value":"0"},"penalties":{"type":"String","label":"Penalties","value":""},"corruption":{"type":"Number","label":"Corruption","value":0},"encumbrance":{"type":"Number","label":"Encumbrance","max":1,"current":0},"mount":{"id":"","mounted":false,"isToken":false,"tokenData":{"scene":"","token":""}}},"details":{"species":{"type":"String","label":"Species","value":"Pigeon","subspecies":""},"gender":{"type":"String","label":"Gender","value":"Varies"},"biography":{"type":"String","label":"Biography","value":"<section id=\"secret-Xfobvpz375Ivv2Vq\" class=\"secret\"><p>Pigeons are bred across the Reikland to carry messages of all kinds. Recently, they have become&nbsp; renowned for carrying a deadlier cargo, as the easily accessible bird has become a favourite amongst engineers, used to deploy their ‘pigeon bombs’ to varying degrees of success.</p></section>"},"gmnotes":{"type":"String","label":"GM Notes","value":""},"size":{"type":"String","label":"Size","value":"tiny"},"move":{"type":"Number","label":"Movement","value":2,"walk":4,"run":8},"god":{"type":"String","label":"God","value":""},"status":{"type":"String","value":"","standing":"","tier":0,"modifier":0},"hitLocationTable":{"value":"hitloc","type":"String","label":"Hit Location Table"}},"excludedTraits":["6yxv7Ob2UIKIDP7L","kX8ZgJMaK1bHUx6A","vE0tNHtWf4Ois87H"]},"prototypeToken":{"flags":{},"name":"Pigeon","displayName":20,"width":1,"height":1,"lockRotation":false,"rotation":0,"actorLink":false,"disposition":0,"displayBars":20,"bar1":{"attribute":"status.wounds"},"bar2":{"attribute":"status.advantage"},"randomImg":false,"alpha":1,"light":{"dim":0,"bright":0,"angle":360,"alpha":1,"animation":{"speed":5,
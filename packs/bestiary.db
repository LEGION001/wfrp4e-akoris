strong> Test or gain a @Condition[Stunned] Condition.</p><p><strong>Lore:</strong> All spells cast from the Lore of Shadows inflicting Damage ignore all non-magical Armor Points.</p><p></p>"},"gmdescription":{"type":"String","label":"Description","value":""},"lore":{"type":"String","label":"Lore","value":"shadow","effectString":"","effect":""},"range":{"type":"String","label":"Range","value":"Willpower yards","vortex":false},"target":{"type":"String","label":"Target","value":"Willpower Bonus yards","aoe":true},"duration":{"type":"String","label":"Duration","value":"Willpower Bonus rounds","extendable":false},"damage":{"type":"String","label":"Damage","dice":"","value":""},"cn":{"type":"Number","label":"Casting Number","value":6,"SL":0},"magicMissile":{"type":"Boolean","label":"Magic Missile","value":false},"ritual":{"value":false,"type":"","xp":0},"memorized":{"type":"Boolean","label":"Memorized","value":false},"skill":{"value":""},"ingredients":[],"currentIng":{"type":"Number","label":"Ingredient","value":""},"wind":{"value":""},"overcast":{"enabled":false,"label":"","valuePerOvercast":{"type":"","value":1,"SL":false,"characteristic":"","bonus":false},"initial":{"type":"","value":1,"SL":false,"characteristic":"","bonus":false}}},"type":"spell","folder":"X6VvvDu2dmvYsCDu","img":"modules/wfrp4e-core/icons/spells/mystifying-miasma.png","effects":[{"label":"Mystifying Miasma","icon":"modules/wfrp4e-core/icons/spells/mystifying-miasma.png","transfer":false,"_id":"9h0rxsBrfte4QnO7","changes":[],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"origin":null,"tint":null,"flags":{"wfrp4e":{"effectTrigger":"oneTime","effectApplication":"apply","hide":false,"preventDuplicateEffects":false,"script":"args.actor.addCondition(\"blinded\")\nargs.actor.addCondition(\"deafened\")\nargs.actor.addCondition(\"fatigued\")"}}}],"sort":3300000,"ownership":{"default":0,"Oxrm36DvLya7eQ7D":3,"36O01TtCfYv